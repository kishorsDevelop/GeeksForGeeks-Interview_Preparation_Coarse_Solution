#include <iostream>
#include <algorithm>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    int result = a[0];
	    for(int i=1;i<n;i++){
	        result = __gcd(result,a[i]);
	    }
	    cout<<result<<endl;
	}
	return 0;
}
